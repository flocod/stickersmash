{
    "build": {
      "development": {
        "android": {
          "buildType": "app-bundle"
        },
        "ios": {
          "distribution": "internal"
        }
      },
      "preview": {
        "android": {
          "buildType": "app-bundle" 
        },
        "ios": {
          "distribution": "internal"
        }
      },
      "production": {
        "android": {
          "buildType": "app-bundle"
        },
        "ios": {
          "distribution": "store"
        }
      }
    },
    "submit": {
      "android": {
        "serviceAccountKeyPath": "./path/to/your/android-service-account-key.json",
        "track": "production"
      },
      "ios": {
        "appleId": "your-apple-id@example.com",
        "ascAppId": "your-app-store-connect-app-id"
      }
    }
  }
  